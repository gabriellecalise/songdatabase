/* name this database: sockmarket */

CREATE TABLE songs (
    id INTEGER PRIMARY KEY AUTO_INCREMENT,
    title TEXT(50),
    artist TEXT(50),
    genre TEXT(50),
    mood TEXT(50),

    );





